{
  "version": 2,
  "name": "ai-flower-recognition",
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "functions": {
    "api/identify.js": {
      "maxDuration": 30
    }
  },
  "env": {
    "ZHIPU_API_KEY": "@zhipu_api_key"
  }
}
